{
  "expo": {
    "name": "Dukaan",
    "slug": "mechanic",
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#FFFFFF"
    },
    "version": "2.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to get the location of the shop you want to create."
        }
      ]
    ],
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dukaan.mechanicc",
      "buildNumber": "4",
      "purposeStrings": {
        "NSLocationAlwaysUsageDescription": "Allow $(PRODUCT_NAME) to get the location of the shop you want to create.",
        "NSLocationWhenInUseUsageDescription": "Allow $(PRODUCT_NAME) to set shop location when creating a shop"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ],
      "package": "com.dukaan.mechanic",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyA47XXKuNkOS0Z6m0RdyKsRgw9ydRr35ww"
        }
      }
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "b60361dc-4b09-4f03-b2a1-71fced05d531"
      }
    }
  }
}
