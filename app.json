{
  "expo": {
    "name": "Dukaan",
    "slug": "mechanic",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dukaan.mechanic",
      "podfile": {
        "modularHeaders": true
      }
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCm1NrKo7E44lu9k_hqjKqyu2QlhiCK38E"
        }
      },
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ],
      "package": "com.dukaan.mechanic"
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "extra": {
      "googleMapsApiKey": "AIzaSyCm1NrKo7E44lu9k_hqjKqyu2QlhiCK38E",
      "eas": {
        "projectId": "52b02931-5a0e-4560-891a-175adc93f923"
      }
    }
  }
}